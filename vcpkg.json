{
  "name": "onscripter-en",
  "version-string": "0.1.0-dev",
  "dependencies": [
    "bzip2",
    {
      "name": "dirent",
      "platform": "(windows)&(!mingw)"
    },
    {
      "name": "ffmpeg",
      "features": [ "avcodec", "avdevice", "avfilter", "avformat", "openh264" ],
      "platform": "(windows|osx|ios|linux)&(!mingw)"
    },
    "freetype",
    {
      "name": "libjpeg-turbo",
      "platform": "!emscripten"
    },
    {
      "name": "libmad",
      "platform": "windows|osx|ios|linux|emscripten"
    },
    "libpng",
    "lua",
    {
      "name": "sdl2",
      "features": [ "base", "vulkan" ],
      "platform": "windows|osx|ios|android|emscripten"
    },
    {
      "name": "sdl2",
      "features": [ "base", "vulkan", "wayland", "x11" ],
      "platform": "linux"
    },
    {
      "name": "sdl2-image",
      "features": [ "libjpeg-turbo" ],
      "platform": "!emscripten"
    },
    {
      "name": "sdl2-image",
      "platform": "emscripten"
    },
    {
      "name": "sdl2-mixer",
      "features": [ "mpg123", "fluidsynth" ],
      "platform": "(windows|linux|osx|ios|android)&(!mingw)"
    },
    {
      "name": "sdl2-mixer",
      "features": [ "mpg123" ],
      "platform": "mingw"
    },
    {
      "name": "sdl2-mixer",
      "platform": "emscripten"
    },
    "sdl2-ttf",
    {
      "name": "smpeg2",
      "platform": "(windows|osx|ios|linux|android)"
    },
    "zlib"
  ]
}
